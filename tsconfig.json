{
  "compilerOptions": {
    "removeComments": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "paths": {
      "@utils/*": ["utils/*"],
      "@models/*": ["models/*"],
      "@factory/*": ["factory/*"],
      "@services/*": ["services/*"],
      "@interfaces/*": ["interfaces/*"],
      "@controllers/*": ["controllers/*"],
    }
  }
}
